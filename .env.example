# Claude Usage Proxy Configuration
# Copy to .env and fill in values

# Render proxy URL
CLAUDE_USAGE_PROXY_URL=https://claude-usage-proxy.onrender.com

# Proxy authentication token
CLAUDE_USAGE_PROXY_TOKEN=your-token-here

# State file path (optional, defaults to ~/.openclaw/workspace/memory/claude-usage-state.json)
CLAUDE_USAGE_STATE_FILE=

# Weekly budget in USD (optional, defaults to 625)
CLAUDE_USAGE_WEEKLY_BUDGET=625
